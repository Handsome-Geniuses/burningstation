{
    "code-runner.executorMap": {
        "python": "PYTHONPATH=~/repos/burningstation/flask python -u",
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/.env": true,
        "**/dist": true,
        "**/__pycache__": true,
        "**/.vscode": true,
        "**/.devcontainer": true,
        "logs": true,
        "docker-compose.yml": true,
        "Dockerfile": true,
        "**/*tempCodeRunnerFile*": true,
        "rebooter.py": true,
        "requirements.txt": true,
        ".gitignore": true,
        "venv": true
    },
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
}