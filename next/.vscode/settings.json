{
    "code-runner.executorMap": {
        // "typescript" : "env $(cat .env | xargs) npx ts-node --compiler-options '{\"module\":\"commonjs\",\"baseUrl\":\"./src\",\"paths\":{\"@/*\":[\"*\"]}}' $fullFileName",
        "typescript" : "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' $fullFileName",
        "javascript" : "PATH=/usr/sbin:$PATH node $fullFileName",
    },
    "files.exclude": {
        "**/.vscode": true,
        "**/node_modules": true,
        "**/src/index.css": true,
        "**/src/main.tsx": true,
        "**/src/vite-env.d.ts": true,
        "**/.gitignore": true,
        "**/index.html": true,
        "**/package-lock.json": true,
        "**/package.json": true,
        "**/README.md": true,
        "**/eslint.config.js": true,
        "**/tsconfig.app.json": true,
        "**/tsconfig.json": true,
        "**/tsconfig.node.json": true,
        "**/vite.config.ts": true,
        "**/src/assets/fonts": true,
        "**/.next": true,
        ".env": true,
        "components.json": true,
        "eslint.config.mjs": true,
        "next-env.d.ts": true,
        "next.config.ts": true,
        "postcss.config.mjs": true,
        "**/favicon.ico": true,
        "public/": true,
        "src/app/globals.css": true,
        "src/app/layout.tsx": true,
        "src/app/loading.tsx": true,
        "src/app/page.tsx": true,
        "src/types/": true,
        "LICENSE": true,
        "drizzle/": true,
        "docker-compose.yml": true,
        "Dockerfile": true,
        "tailwind.config.js": true,
        ".devcontainer/": true
    },
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
}