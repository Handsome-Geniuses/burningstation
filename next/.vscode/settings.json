{
    "code-runner.executorMap": {
        // "typescript" : "env $(cat .env | xargs) npx ts-node --compiler-options '{\"module\":\"commonjs\",\"baseUrl\":\"./src\",\"paths\":{\"@/*\":[\"*\"]}}' $fullFileName",
        "typescript" : "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' $fullFileName",
        "javascript" : "PATH=/usr/sbin:$PATH node $fullFileName",
    },
    "files.exclude": {
        "**/.vscode": false,
        "**/node_modules": false,
        "**/src/index.css": false,
        "**/src/main.tsx": false,
        "**/src/vite-env.d.ts": false,
        "**/.gitignore": false,
        "**/index.html": false,
        "**/package-lock.json": false,
        "**/package.json": false,
        "**/README.md": false,
        "**/eslint.config.js": false,
        "**/tsconfig.app.json": false,
        "**/tsconfig.json": false,
        "**/tsconfig.node.json": false,
        "**/vite.config.ts": false,
        "**/src/assets/fonts": false,
        "**/.next": false,
        ".env": false,
        "components.json": false,
        "eslint.config.mjs": false,
        "next-env.d.ts": false,
        "next.config.ts": false,
        "postcss.config.mjs": false,
        "**/favicon.ico": false,
        "public/": false,
        "src/app/globals.css": false,
        "src/app/layout.tsx": false,
        "src/app/loading.tsx": false,
        "src/app/page.tsx": false,
        "src/types/": false,
        "LICENSE": false,
        "drizzle/": false,
        "docker-compose.yml": false,
        "Dockerfile": false,
        "tailwind.config.js": false,
        ".devcontainer/": false
    },
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
}